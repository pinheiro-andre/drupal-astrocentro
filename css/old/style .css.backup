	);/* Saf4+, Chrome */
	filter:  progid:DXImageTransform.Microsoft.gradient(startColorStr='#19FFFFFF', EndColorStr='#19000000'); /* IE6,IE7 */
	-ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorStr='#19FFFFFF', EndColorStr='#19000000')"; /* IE8 */
	-moz-user-select: none;
	-webkit-user-select:none;
	-khtml-user-select: none;
	user-select: none;
	margin-bottom:10px;
}
.butao.full, .butao.full span {
	display: block;
}
.butao:hover, .butao.hover {
	background: -moz-linear-gradient(
		center top,
		rgba(255, 255, 255, .2) 0%,
		rgba(255, 255, 255, .1) 100%
	);/* FF3.6 */
	background: -webkit-gradient(
		linear,
		center bottom,
		center top,
		from(rgba(255, 255, 255, .1)),
		to(rgba(255, 255, 255, .2))
	);/* Saf4+, Chrome */
	filter:  progid:DXImageTransform.Microsoft.gradient(startColorStr='#33FFFFFF', EndColorStr='#19FFFFFF'); /* IE6,IE7 */
	-ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorStr='#33FFFFFF', EndColorStr='#19FFFFFF')"; /* IE8 */
}
.butao:active, .butao.active {
	top:1px;
}
.butao span {
	position: relative;
	color:#fff;
	text-shadow:0 1px 1px rgba(0, 0, 0, 0.25);
	border-top: rgba(255, 255, 255, .2) 1px solid;
	padding:0.6em 1.3em;
	line-height:1em;
	text-decoration:none;
	text-align:center;
	white-space: nowrap;
}
.butao span i {
	margin-right:1.3em;
}

.butao.small span {
	font-size:12px;
}
.butao.medium span {
	font-size:14px;
}
.butao.large span {
	font-size:18px;
}

.butao.bordeaux {
	background-color: #9F0D5E;
}
.butao.pink {
	background-color: #CB00B1;
}

/* error message LiveValidation */
span.LV_valid, span.LV_valid:before, span.LV_valid:after{display:none;}

span.LV_validation_message{
	border-radius:5px;
	color:white;
	font-size:12px;
	padding:4px;
	position:absolute;
	z-index: 5;
	background-color:#BF2600;
	right:-70px;
	top:-33px;
	box-shadow:0 1px 2px rgba(0,0,0,.5);
}
span.LV_validation_message:before,
span.LV_validation_message:after{
	border-color:#BF2600 transparent transparent transparent;
	border-style:solid;
	border-width:8px 8px 0 8px;
	content:"";
	right:8px;
	position:absolute;
	top:27px;
	z-index:3;
}
div.form-item-prospect-email > span.LV_validation_message{margin:0 !important; right:0; top:-33px;}


