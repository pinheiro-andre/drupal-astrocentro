/*********************************************************************************************** Expert tabs */
#block-astrocentro-br-app-astrocentro-br-experts-tabs{
	padding: 5px 0 !important;
}
.expert-container{
	width: 100%;
	margin: 10px auto;
	text-align: left;
}
.expert-container label{
	text-align: center;
	padding: 5px 20px;
	position: relative;
	z-index: 20;
	display: block;
	height: 30px;
	cursor: pointer;
	color: #777;
	text-shadow: 1px 1px 1px rgba(255,255,255,0.8);
	line-height: 33px;
	font-size: 19px;
	background: #f8f7ee;
	padding: 20px;
}
.expert-container label:hover{
	background: #fff;
}
.expert-container input:checked + label,
.expert-container input:checked + label:hover{
	background-color: #ffffff !important;
	box-shadow: inset 0 2px 0 #9F0D5E;
	color: #29324e;
}
.expert-container label:hover:after,
.expert-container input:checked + label:hover:after{
	content: '';
	position: absolute;
	width: 24px;
	height: 24px;
	right: 13px;
	top: 25px;
	background: transparent url(images/arrow_down.png) no-repeat center center;	
}
.expert-container input:checked + label:hover:after{
	background-image: url(images/arrow_up.png);
}
.expert-container input{
	display: none;
}
.expert-container article{
	background: rgba(255, 255, 255, 0.5);
	margin-top: -1px;
	overflow: hidden;
	height: 0px;
	position: relative;
	z-index: 10;
	-webkit-transition: height 0.3s ease-in-out, box-shadow 0.6s linear;
	-moz-transition: height 0.3s ease-in-out, box-shadow 0.6s linear;
	-o-transition: height 0.3s ease-in-out, box-shadow 0.6s linear;
	-ms-transition: height 0.3s ease-in-out, box-shadow 0.6s linear;
	transition: height 0.3s ease-in-out, box-shadow 0.6s linear;
}
.expert-container article p{
	font-style: italic;
	color: #777;
	line-height: 23px;
	font-size: 14px;
	padding: 20px;
	text-shadow: 1px 1px 1px rgba(255,255,255,0.8);
}
.expert-container input:checked ~ article{
	-webkit-transition: height 0.5s ease-in-out, box-shadow 0.1s linear;
	-moz-transition: height 0.5s ease-in-out, box-shadow 0.1s linear;
	-o-transition: height 0.5s ease-in-out, box-shadow 0.1s linear;
	-ms-transition: height 0.5s ease-in-out, box-shadow 0.1s linear;
	transition: height 0.5s ease-in-out, box-shadow 0.1s linear;
}
.expert-container input:checked ~ article.expert-contenu{
	height: 590px;
}
.expert-container article.expert-contenu{
	padding: 0 10px;
}