/*-------------------------------------------*
/* Jcarousel skin
/*-------------------------------------------*/
.jcarousel-skin-astrobr {
  text-align: center;
}

.jcarousel-skin-astrobr .jcarousel-container-horizontal {
  width: 100%;
  height: 220px !important;
  padding: 15px;
  margin: auto;
}

.jcarousel-skin-astrobr .jcarousel-clip-horizontal {
  width: 100%;
  overflow: hidden;
}

.jcarousel-skin-astrobr .jcarousel-item {
	padding: 0;
	width: 190px;
	height: 170px;
	overflow: hidden;
	border-radius: 10px;
	border-bottom: 4px solid #e2e3e7;
	list-style: none;
	background: #fff none;
}

.jcarousel-skin-astrobr .jcarousel-item-horizontal {
  margin-right: 32px;}
  
	.jcarousel-skin-astrobr .jcarousel-item-horizontal .views-field-field-video-url-embed{
		position:relative;}
		
		.jcarousel-skin-astrobr .jcarousel-item-horizontal .views-field-field-video-url-embed a:before{
			position:absolute;
			font-family: FontAwesome;
			color: rgba(255, 255, 255, 0.8);
			top:15%;
			right:35%;
			content: "\f16a";
			font-size:3em;}
			
			.jcarousel-skin-astrobr .jcarousel-item-horizontal .views-field-field-video-url-embed a:hover:before{
				color: rgba(255, 255, 255, 1);
				transition:all 0.5s ease;}
				
		.jcarousel-skin-astrobr .jcarousel-item-horizontal .views-field-title a{
			padding:2px;}
			
		.jcarousel-skin-astrobr .jcarousel-item-horizontal img{
			padding:0 !important;
			border-top-left-radius:10px !important;
			border-top-right-radius:10px !important;
			border-bottom-left-radius:0 !important;
			border-bottom-left-radius:0 !important;}

.jcarousel-skin-astrobr .jcarousel-item-placeholder {
  background: #fff url(images/jcarousel-skin/throbber.gif) no-repeat center center;
  color: #000;
}

/**
 * Buttons
 */
.jcarousel-skin-astrobr .jcarousel-next,
.jcarousel-skin-astrobr .jcarousel-prev {
  display: block;
  width: 32px;
  height: 32px;
  background-image: url(images/jcarousel-skin/arrows.png);
}

.jcarousel-skin-astrobr .jcarousel-next-disabled,
.jcarousel-skin-astrobr .jcarousel-prev-disabled {
  display: none;
}

/**
 * Horizontal Buttons
 */
.jcarousel-skin-astrobr .jcarousel-prev-horizontal {
  position: absolute;
  bottom: 20px;
  left: 10px;
  background-position: 0 0;
}
.jcarousel-skin-astrobr .jcarousel-prev-horizontal:hover {
  background-position: -32px 0;
}
.jcarousel-skin-astrobr .jcarousel-prev-horizontal:active {
  background-position: -64px 0;
}

.jcarousel-skin-astrobr .jcarousel-next-horizontal {
  position: absolute;
  bottom: 20px;
  right: 45px;
  background-position: 0 -32px;
}
.jcarousel-skin-astrobr .jcarousel-next-horizontal:hover {
  background-position: -32px -32px;
}
.jcarousel-skin-astrobr .jcarousel-next-horizontal:active {
  background-position: -64px -32px;
}

.jcarousel-skin-astrobr .jcarousel-navigation {
  margin: 0;
  padding: 0;
}

.jcarousel-skin-astrobr .jcarousel-navigation li {
  display: inline;
  margin: 0 2px 0 0;
  padding: 0;
  background: none;
}

.jcarousel-skin-astrobr .jcarousel-navigation li.active a {
  font-weight: bold;
  text-decoration: none;
}

