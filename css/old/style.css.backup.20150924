/* Main Navigation
================================================== */


/*hide responsive nav*/
#top-bar .selector,
#navigation .selector {
  display: none;
}
#mobile-header {display: none;}

/*core*/
#main-menu > ul, #main-menu > ul * {
  margin: 0;
  padding: 0;
  list-style: none;
}

#main-menu > ul {
  line-height: 1.0;
}

#main-menu > ul ul {
  position: absolute;
  top: -999em;
  width: 180px;
}

#main-menu > ul ul li {
  width: 101%;
}

#main-menu > ul li:hover {
  visibility: inherit;
}

#main-menu > ul li {
  float: left;
  position: relative;
}

#main-menu > ul a {
  display: block;
  position: relative;
}

#main-menu > ul li:hover ul, #main-menu > ul li.sfHover ul {
  left: 0px;
  top: 30px;
  z-index: 99;
}

#main-menu > ul li:hover li ul, #main-menu > ul li.sfHover li ul {
  top: -999em;
}

#main-menu > ul li li:hover ul, #main-menu > ul li li.sfHover ul {
  left: 180px;
  top: 1px;
}

/*styling*/
#navigation {
  height:48px;
  bottom: 0;
  left: 0;
  width:100%;
  background: url("images/bg-menu-black.gif") repeat-x;
  z-index:1000;
}
#navigation.stuck {
	position:fixed;
	top:0;
	box-shadow:0 2px 4px rgba(0, 0, 0, .3);
}

#navigation #main-menu > ul > li:hover > ul, #main-menu > ul li.sfHover > ul {
  top: 30px;
}

#navigation #main-menu > ul li li:hover ul, #main-menu > ul li li.sfHover ul,
#main-menu > ul li li:hover ul, #main-menu > ul li li.sfHover ul {
  left: 200px;
  top: -10px;
}

#navigation #main-menu > ul a {
  font-size: 1em;
  height: /*32px*/ 40px;
  line-height: 38px;
  color: #fffdf3;
  padding: 0 15px;
  text-decoration: none;
  font-weight: 600;
  /*border-right:1px solid #a3028f;*/
}
#navigation #main-menu > ul a:nth-last-child(-n+1) {border:0;}

#navigation #main-menu > ul li {
  float: left;
  position: relative;
  /*background: url("images/menu-separator.gif") no-repeat;
  height:48px;*/
  padding-top:0;
}

#navigation #main-menu > ul > li:nth-last-child(-n+2){
	background:url("images/bg-menu-pink.gif") repeat-x;
	float:right;
}
#navigation #main-menu > ul > li:nth-last-child(-n+2) > a { padding:0 15px; }
#navigation #main-menu > ul > li:nth-last-child(-n+2) > a:hover { line-height:24px; }

#navigation #main-menu > ul > li > a:hover {
	color: #fff;
	background-color:#9f0d5e;
	line-height: 23px;
	padding-top: 8px;
	height:32px;
}

#navigation #main-menu > ul > li > a.active {
  /*background: #fffdf3;
  color: #000000;
  height:40px;*/
}

#navigation #main-menu > ul > li.sfHover > a,
#navigation a.active,
#navigation a.active:hover,
#navigation #main-menu > ul > li > a.active-trail {
  color: #fffdf3;
}

/*drop-down styles*/
#navigation #main-menu > ul ul {
  position: absolute;
  nowhitespace: afterproperty;
  top: -999em;
  width: 100%;
  z-index: 9999;
  left: 0;
  padding-top: 10px;
  border-radius: 2px;
}

#navigation #main-menu > ul ul > li:first-child{
  /*-webkit-border-top-left-radius: 2px;
  -webkit-border-top-right-radius: 2px;
  -moz-border-radius-topleft: 2px;
  -moz-border-radius-topright: 2px;
  border-top-left-radius: 2px;
  border-top-right-radius: 2px;*/
}
#navigation #main-menu > ul ul > li:last-child,
#navigation #main-menu > ul ul > li:last-child:hover{
  -webkit-border-bottom-right-radius: 5px;
  -webkit-border-bottom-left-radius: 5px;
  -moz-border-radius-bottomright: 5px;
  -moz-border-radius-bottomleft: 5px;
  border-bottom-right-radius: 5px;
  border-bottom-left-radius: 5px;
}
#navigation #main-menu > ul ul li {
  background: #000;
  border-bottom: 1px solid #000;
}

#navigation #main-menu > ul ul li:first-child {
  border-top: 0;
}

/*#navigation #main-menu > ul ul li:last-child {
  border-bottom: 0;
}*/

#navigation #main-menu > ul ul a {
  display: block;
  padding: 8px;
  color: #fff;
  height: auto;
  margin: 0px;
  border: 0px !important;
  font-size: 0.929em;
  font-weight: normal;
  text-shadow: none;
  text-align: center;
}

#navigation #main-menu > ul ul li > a:hover {
  background-color: #9f0d5e;
}

