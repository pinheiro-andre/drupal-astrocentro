}

/*Popup form*/
#fade {
	display: none;
	background: #ffffff;
	position: fixed; left: 0; top: 0;
	width: 100%; height: 100%;
	opacity: .80;
	z-index: 9999;
}
.content > .popup_block{
	display: none;

background: #ffffff;
background: url(data:image/svg+xml;base64,);
background: -moz-linear-gradient(top,  #ffffff 1%, #f7f3ef 100%);
background: -webkit-gradient(linear, left top, left bottom, color-stop(1%,#ffffff), color-stop(100%,#f7f3ef));
background: -webkit-linear-gradient(top,  #ffffff 1%,#f7f3ef 100%);
background: -o-linear-gradient(top,  #ffffff 1%,#f7f3ef 100%);
background: -ms-linear-gradient(top,  #ffffff 1%,#f7f3ef 100%);
background: linear-gradient(to bottom,  #ffffff 1%,#f7f3ef 100%);
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#f7f3ef',GradientType=0 );

	padding: 20px;
	border: 5px solid #d270c5;
	float: left;
	position: fixed;
	top: 50%; left: 50%;
	z-index: 99999;

	-webkit-box-shadow: 0px 0px 20px #000;
	-moz-box-shadow: 0px 0px 20px #000;
	box-shadow: 0px 0px 20px #000;

	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
}
.content > .popup_block > h4{
	font-size:18px;
	font-weeight:100;
	color:#ce11b6;
}

.content > .popup_block > p{
	font-size:14px;
}
img.close_bt {
	float: right;
	margin: -15px -15px 0 0;
}

*html #fade {position: absolute;}
*html .popup_block {position: absolute;}

/*Forms*/
.content > .popup_block > form label{
	font-weight:100 !important;
	color:##929292;
}

@-moz-document url-prefix()
{
    #edit-tdm-birthdate, #edit-tdm-mail, #edit-tdm-name {
        width:370px;
    }
}
html > body #edit-tdm-birthdate,
html > body #edit-tdm-mail,
html > body #edit-tdm-name {
    *width:375px;
}

.content > .popup_block > form .form-submit,
form#astrocentro-br-app-taro-do-amor-step2-form .form-submit{
