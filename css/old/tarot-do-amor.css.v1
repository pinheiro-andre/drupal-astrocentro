/* Styling cards */
#tarot-do-amor ul#index_cards li img {
	margin-top:6px;
	float:left;
}

#tarot-do-amor ul#index_cards li a {
	display:block;
	overflow:hidden;
	height:100%;
    text-decoration: none;
    outline: none;
	clear:both;
	background:url(/sites/all/themes/astrocentro_br/images/etoileamour.png) repeat;
}

/*Chosen cards reveal*/
#tarot-do-amor #readingcards {
	position:relative;
	z-index:100;
	width:100%;
	height:100%;
}

#tarot-do-amor .caption {
	display: inline-block;
	overflow: hidden;
	position: relative;
	width:100%;
	padding:10px;
	margin-top:50px;
}

#tarot-do-amor .caption > img {
	display: block;
	float:left;
	margin-right:20px;
	padding:5px;
	
	box-shadow: 3px 3px 7px rgba(50, 50, 50, 0.75);
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
	
	-moz-border-radius: 10px;
	-webkit-border-radius: 10px;
	-ms-border-radius: 10px;
	border-radius: 10px;
	
	border:3px solid #ffffff;
}
#tarot-do-amor .caption h4{
	font-weight:bold;
	margin-bottom:25px;
	font-size:1.5em;
}
#tarot-do-amor .caption p {
	font-size: 14px;
	padding: 20px;
	text-align:left;
	line-height:1.5em;
}

/*button*/
#content .button-moreinfos {
	border:1px solid #ffffff;
	background-color:#cb00b1;
	padding:8px;
	border-radius:5px;
	width:100%;
	color:#ffffff;
	font-weight:bold;
	cursor:pointer;
	font-size:15px;
}
#content .button-moreinfos:hover {
	border:1px solid #cb00b1;
	background-color:#ffffff;
	color:#cb00b1;
}

.messages .error{
	/*display:none;*/
}

.page-oraculos-tarot-do-amor-gratis #content-wrap .content .final-step3 p{
